{
  "name": "ticketing-lib-ui",
  "version": "2.1.13",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "start:ssl": "ng serve --hmr --ssl",
    "start:prod": "ng serve",
    "build": "ng build",
    "build:app:prod": "ng build",
    "build:lib:dev": "ng build ticketing-lib-ui-library --configuration development",
    "build:lib:prod": "ng build ticketing-lib-ui-library ",
    "build:graphlib:dev": "ng build ticketing-graphic-kit --configuration development",
    "build:graphlib:prod": "ng build ticketing-graphic-kit",
    "build:ci": "run-s build:graphlib:prod build:lib:prod",
    "build:dev": "run-s build:graphlib:dev build:lib:dev",
    "build:dev:full": "run-s build:graphlib:dev build:lib:dev asciidoc",
    "test": "ng test --pass-with-no-tests",
    "test:app": "ng test ticketing-lib-ui --pass-with-no-tests --silent=false",
    "test:lib": "ng test ticketing-lib-ui-library --pass-with-no-tests",
    "test:graphlib": "ng test ticketing-graphic-kit --pass-with-no-tests",
    "asciidoc": "docker-compose -f ./devenv/docker-compose.doc.yml --profile generate-documentation up",
    "jest": "jest --config jest.base.config.mjs",
    "jest:lib": "jest --config projects/ticketing-lib-ui-library/jest.config.ts",
    "jest:graphlib": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config projects/ticketing-graphic-kit/jest.config.mjs",
    "lint": "ng lint",
    "lint:staged": "lint-staged",
    "format:check": "pretty-quick --check",
    "format:fix": "pretty-quick",
    "format:fix:staged": "pretty-quick --staged",
    "e2e": "ng e2e",
    "docs:html": "compodoc -p ./.compodoc/tsconfig.compodoc.json -d ./.compodoc/docs -s",
    "docs:json": "compodoc -p ./.compodoc/tsconfig.compodoc.storybook.json -e json -d ./.compodoc/docs",
    "storybook": "yarn docs:json && start-storybook -p 6006",
    "storybook:build": "yarn docs:json && build-storybook",
    "mock:backend": "cp mock-backend/init-db.json mock-backend/db.json && npx json-server mock-backend/db.json --routes mock-backend/routes.json  --port 3000",
    "postinstall": "ngcc"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/localize": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@ngneat/transloco": "~4.1.1",
    "rxjs": "~7.5.6",
    "tslib": "2.4.0",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-builders/jest": "~14.0.0",
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@ngneat/spectator": "~11.1.3",
    "@types/jest": "~27.5.2",
    "@types/node": "^16.11.47",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-esm-transformer": "^1.0.0",
    "jest-preset-angular": "^12.2.0",
    "jest-sonar": "~0.2.12",
    "ng-mocks": "~14.1.0",
    "ng-packagr": "^14.1.0",
    "ts-jest": "~28.0.7",
    "ts-node": "~10.9.1",
    "typescript": "~4.6.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": ["<rootDir>/setup-jest-esm.ts"],
    "globalSetup": "jest-preset-angular/global-setup.mjs"
  }
}